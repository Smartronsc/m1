<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

    <display-name>Smartronsc Created Web Application</display-name>
    <listener>
       <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>

<servlet>
    <servlet-name>CompaniesServlet</servlet-name>
    <servlet-class>com.androidcommand.app.CompaniesServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>CompaniesServlet</servlet-name>
    <url-pattern>/companies</url-pattern>
</servlet-mapping>

</web-app>

